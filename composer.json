{
    "name": "gupalo/dateutils",
    "description": "DateTime management",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.2"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Gupalo\\DateUtils\\": "src/DateUtils/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\Tests\\DateUtils\\": "tests/DateUtils/"
        }
    }
}
